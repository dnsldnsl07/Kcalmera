    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_management">

    <fragment
        android:id="@+id/navigation_camera"
        android:name="com.example.kcalmera.ui.cam.CamFragment"
